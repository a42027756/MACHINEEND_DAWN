+using System;
+using System.Collections;
+using System.Collections.Generic;
+using UnityEngine;
+
+public class ReleasePeople : MonoBehaviour
+{
+    // Start is called before the first frame update
+    void Start()
+    {
+        
+    }
+
+    // Update is called once per frame
+    void Update()
+    {
+        
+    }
+
+    private void OnTriggerEnter2D(Collider2D other)
+    {
+        if (other.CompareTag("Player"))
+        {
+            GameObject.Find("WWhiteVillager").GetComponent<Villager>().target = null;
+            GameObject.Find("GreenVillager").GetComponent<Villager>().target = null;
+            GameObject.Find("PurpleVillager").GetComponent<Villager>().target = null;
+            GameManager.Instance.OpenDoor();
+        }
+    }
+}
